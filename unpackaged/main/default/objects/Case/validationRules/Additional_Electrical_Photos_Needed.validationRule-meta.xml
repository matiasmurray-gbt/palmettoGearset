<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Electrical_Photos_Needed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
     ISCHANGED (Additional_Electrical_Photos_Needed_Date__c),
     ISBLANK(Additional_Electrical_Photos_Needed_Note__c)
)</errorConditionFormula>
    <errorDisplayField>Additional_Electrical_Photos_Needed_Note__c</errorDisplayField>
    <errorMessage>Additional Electrical Photos Needed Note cannot be blank</errorMessage>
</ValidationRule>
