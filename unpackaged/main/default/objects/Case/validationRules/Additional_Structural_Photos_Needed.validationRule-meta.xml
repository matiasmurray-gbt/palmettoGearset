<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Structural_Photos_Needed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISCHANGED (Additional_Structural_Photos_Needed_Date__c),
ISBLANK(Additional_Structural_Photos_Needed_Note__c)
)</errorConditionFormula>
    <errorDisplayField>Additional_Structural_Photos_Needed_Note__c</errorDisplayField>
    <errorMessage>Additional Structural Photos Needed Note cannot be blank</errorMessage>
</ValidationRule>
