<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Roof_Photos_Needed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISCHANGED (Additional_Roof_Photos_Needed_Date__c),
ISBLANK(Additional_Roof_Photos_Needed_Notes__c)
)</errorConditionFormula>
    <errorDisplayField>Additional_Roof_Photos_Needed_Notes__c</errorDisplayField>
    <errorMessage>Additional Roof Photos Needed Notes cannot be blank</errorMessage>
</ValidationRule>
