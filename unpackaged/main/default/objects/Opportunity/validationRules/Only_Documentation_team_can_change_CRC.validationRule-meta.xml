<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Only_Documentation_team_can_change_CRC</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISCHANGED( Contract_Received_Confirmed__c ),
$UserRole.Id  &lt;&gt; "00E2L000000t8Ju",
$UserRole.Id  &lt;&gt; "00E1a000000QAZZ")</errorConditionFormula>
    <errorMessage>Only Documentation team can change Contract Received Confirmed</errorMessage>
</ValidationRule>
